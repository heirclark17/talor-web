{
  "expo": {
    "name": "Talor",
    "slug": "talor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "talor",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.talor.app",
      "infoPlist": {
        "UIBackgroundModes": [],
        "NSDocumentsFolderUsageDescription": "Allow Talor to access documents to upload your resume",
        "NSPhotoLibraryUsageDescription": "Allow Talor to access photos",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.talor.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-document-picker",
      "expo-system-ui",
      "expo-font",
      [
        "@clerk/clerk-expo/plugin",
        {
          "ios": {
            "bundleIdentifier": "com.talor.app"
          },
          "android": {
            "packageName": "com.talor.app"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "04d139f0-2456-4fbb-8115-84c8749f3dc4"
      }
    },
    "owner": "heirclarks-organization",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/04d139f0-2456-4fbb-8115-84c8749f3dc4"
    }
  }
}

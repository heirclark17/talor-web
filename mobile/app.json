{
  "expo": {
    "name": "Talor",
    "slug": "talor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "talor",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.talor.app",
      "infoPlist": {
        "UIBackgroundModes": [],
        "NSDocumentsFolderUsageDescription": "Allow Talor to access documents to upload your resume",
        "NSPhotoLibraryUsageDescription": "Allow Talor to access photos"
      }
    },
    "android": {
      "package": "com.talor.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-document-picker"
    ]
  }
}
